<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.StuAnswer" table="db_answer" lazy="false">
		<id name="id" type="integer">
			<!-- 主键生成策略 -->
			<generator class="native"></generator>
		</id>
		<property name="stID" not-null="true"></property>
		<property name="studentID" not-null="true"></property>
		<property name="stTitle" not-null="true"></property>
		<property name="stParse" not-null="true"></property>
		<property name="stuAns" not-null="true"></property>
	</class>
</hibernate-mapping>
